<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#ffffff"
    android:layout_height="match_parent"
    tools:context=".Facility3">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:textColor="@color/colorPrimary"
            android:textStyle="bold"
            android:textSize="22dp"
            android:text="Facilities by Us "
            android:typeface="serif"
            android:layout_gravity="center"
            android:layout_height="wrap_content" />

        <View
            android:layout_height="1dp"
            android:layout_width="190dp"
            android:layout_marginTop="3dp"
            android:layout_gravity="center"
            android:background="@color/colorPrimary">
        </View>
        <View
            android:layout_height="1dp"
            android:layout_width="190dp"
            android:layout_marginTop="2dp"
            android:layout_gravity="center"
            android:background="@color/colorPrimary">
        </View>


        <LinearLayout
            android:layout_marginTop="22dp"
            android:orientation="horizontal"
            android:layout_marginBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="135dp">
            <android.support.v7.widget.CardView
                android:clickable="true"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginRight="5dp"
                android:layout_width="0dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/image1"
                        android:layout_centerInParent="true"
                        android:src="@drawable/landscapedgarden"
                        android:layout_width="140dp"
                        android:layout_height="80dp" />
                    <TextView
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:text="Garden"
                        android:textColor="@color/colorPrimary"
                        android:typeface="serif"
                        android:textSize="16sp"
                        android:layout_below="@+id/image1"
                        android:layout_centerHorizontal="true"
                        android:layout_height="wrap_content" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_weight="1"
                android:gravity="center"
                android:clickable="true"
                android:layout_width="0dp"
                android:layout_marginLeft="5dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/image2"
                        android:layout_centerInParent="true"
                        android:src="@drawable/joggingtrack"
                        android:layout_width="140dp"
                        android:layout_height="80dp" />
                    <TextView
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:text="Jogging Track"
                        android:textColor="@color/colorPrimary"
                        android:typeface="serif"
                        android:textSize="16sp"
                        android:layout_below="@+id/image2"
                        android:layout_centerHorizontal="true"
                        android:layout_height="wrap_content" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_marginBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="135dp">
            <android.support.v7.widget.CardView
                android:layout_weight="1"
                android:gravity="center"
                android:clickable="true"
                android:id="@+id/idAsp"
                android:layout_marginRight="5dp"
                android:layout_width="0dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/image3"
                        android:layout_centerInParent="true"
                        android:src="@drawable/indoor"
                        android:layout_width="140dp"
                        android:layout_height="80dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:text="Indoor Games"
                        android:textColor="@color/colorPrimary"
                        android:typeface="serif"
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_below="@+id/image3"
                        android:layout_centerHorizontal="true"
                        android:layout_height="wrap_content" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_weight="1"
                android:gravity="center"
                android:clickable="true"
                android:id="@+id/idNdt"
                android:layout_width="0dp"
                android:layout_marginLeft="5dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/image4"
                        android:layout_centerInParent="true"
                        android:src="@drawable/intercom"
                        android:layout_width="140dp"
                        android:layout_height="80dp" />
                    <TextView
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:text="Intercom"
                        android:textColor="@color/colorPrimary"
                        android:typeface="serif"
                        android:textSize="16sp"
                        android:layout_below="@+id/image4"
                        android:layout_centerHorizontal="true"
                        android:layout_height="wrap_content" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_marginBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="135dp">
            <android.support.v7.widget.CardView
                android:layout_weight="1"
                android:gravity="center"
                android:clickable="true"
                android:id="@+id/idglassclening"
                android:layout_marginRight="5dp"
                android:layout_width="0dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/image5"
                        android:layout_centerInParent="true"
                        android:src="@drawable/gym"
                        android:layout_width="140dp"
                        android:layout_height="80dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:text="Gymnasium"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"
                        android:typeface="serif"
                        android:textSize="16sp"
                        android:layout_below="@+id/image5"
                        android:layout_centerHorizontal="true"
                        android:layout_height="wrap_content" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_weight="1"
                android:gravity="center"
                android:layout_width="0dp"
                android:clickable="true"
                android:id="@+id/idretrofitting"
                android:layout_marginLeft="5dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/image6"
                        android:layout_centerInParent="true"
                        android:src="@drawable/elevator"
                        android:layout_width="140dp"
                        android:layout_height="80dp" />
                    <TextView
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:text="Lift"
                        android:textColor="@color/colorPrimary"
                        android:typeface="serif"
                        android:textSize="16sp"
                        android:layout_below="@+id/image6"
                        android:layout_centerHorizontal="true"
                        android:layout_height="wrap_content" />


                </RelativeLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_marginBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="135dp">
            <android.support.v7.widget.CardView
                android:clickable="true"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginRight="5dp"
                android:layout_width="0dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/image27"
                        android:layout_centerInParent="true"
                        android:src="@drawable/security"
                        android:layout_width="140dp"
                        android:layout_height="80dp" />
                    <TextView
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:text="Security"
                        android:textColor="@color/colorPrimary"
                        android:typeface="serif"
                        android:textSize="16sp"
                        android:layout_below="@+id/image27"
                        android:layout_centerHorizontal="true"
                        android:layout_height="wrap_content" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_weight="1"
                android:gravity="center"
                android:clickable="true"
                android:layout_width="0dp"
                android:layout_marginLeft="5dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/image29"
                        android:layout_centerInParent="true"
                        android:src="@drawable/cctv"
                        android:layout_width="140dp"
                        android:layout_height="80dp" />
                    <TextView
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:text="CCTV"
                        android:textColor="@color/colorPrimary"
                        android:typeface="serif"
                        android:textSize="16sp"
                        android:layout_below="@+id/image29"
                        android:layout_centerHorizontal="true"
                        android:layout_height="wrap_content" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>





    </LinearLayout>


</RelativeLayout>
</ScrollView>
